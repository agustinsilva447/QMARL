#include <stdio.h>
#include "comblock.h"
#include "platform.h"
#include "xil_printf.h"
#include "sleep.h"
#include "xparameters.h"


int main()
{
    init_platform();
	short int ax1, ay2, ax3, bx1, by2, bx3;

	ax1 = 1;
	ay2 = 2;
	ax3 = 3;
	bx1 = 13;
	by2 = 14;
	bx3 = 15;

    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG0, (ax1));
    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG1, (ay2));
    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG2, (ax3));
    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG3, (bx1));
    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG4, (by2));
    cbWrite(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_OREG5, (bx3));

    sleep(1);

    p_00 = (cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG0));
    p_01 = (cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG1));
    p_10 = (cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG2));
    p_11 = (cbRead(XPAR_COMBLOCK_0_AXIL_BASEADDR, CB_IREG3));

    sleep(1);

    print("Hello World\n");
    cleanup_platform();
    return 0;
}
